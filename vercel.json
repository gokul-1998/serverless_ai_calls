{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/explain.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/explain",
      "dest": "/api/explain.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
